{
  "name": "validations-br",
  "version": "1.4.0",
  "packageManager": "pnpm@10.15.1",
  "description": "A validator to BR informations",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/reactivando/validations-br.git",
  "homepage": "https://github.com/reactivando/validations-br#readme",
  "author": "Reactivando <reactivando2@gmail.com>",
  "keywords": [
    "BR",
    "Brazil",
    "Validator"
  ],
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "test": "jest",
    "test:e2e": "pnpm build && cd examples && pnpm install && pnpm start",
    "release": "release-it"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "ts-jest": "^29.4.1",
    "tsdown": "^0.15.0",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,json,jsx,ts,tsx}": [
      "eslint",
      "prettier --check"
    ]
  }
}
